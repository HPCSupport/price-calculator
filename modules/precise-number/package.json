    "@types/node": "^10.17.13",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "lodash": "^4.17.15"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}