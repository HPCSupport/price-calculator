{
  "name": "@asset-vector/precise-number",
  "version": "0.0.0",
  "api": "0",
  "feature": "0.0",
  "main": "main.ts",
  "license": "MIT",
  "scripts": {
    "test": "yarn run jest",
    "build": "yarn run gulp",
    "serve": "ts-node main.ts",
    "install-build": "yarn install && yarn run gulp",
    "install-test": "yarn install && yarn test",
    "commit-major": "yarn test && yarn version --major --no-git-tag-version && git add ./*",
