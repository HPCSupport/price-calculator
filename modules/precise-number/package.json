    "commit-patch": "yarn test && yarn version --patch --no-git-tag-version && git add ./*"
  },
  "devDependencies": {
    "@asset-vector/build-helper": "0.1.0",
    "@types/gulp": "^4.0.6",
    "@types/jest": "^24.0.11",
